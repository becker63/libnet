{
  "id": null,
  "uid": "fuzz-monitor",
  "title": "LibFuzzer Harness Metrics (Multi-Core + Aggregate)",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 6,
  "refresh": "500ms",
  "time": { "from": "now-10m", "to": "now" },
  "templating": {
    "list": [
      {
        "name": "core",
        "label": "Core",
        "type": "query",
        "datasource": "prometheus",
        "refresh": 1,
        "query": "label_values(fuzz_executions_total, core)",
        "includeAll": true,
        "multi": true
      }
    ]
  },
  "panels": [
    {
      "type": "row",
      "title": "Aggregate Overview",
      "id": 0,
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "stat",
      "title": "Total Executions (All Cores)",
      "id": 1,
      "gridPos": { "x": 0, "y": 1, "w": 6, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [{ "expr": "sum(fuzz_executions_total)", "refId": "A" }],
      "options": {
        "reduceOptions": { "calcs": ["last"] },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      }
    },
    {
      "type": "stat",
      "title": "Aggregate Exec Speed (execs/sec)",
      "id": 2,
      "gridPos": { "x": 6, "y": 1, "w": 6, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [{ "expr": "sum(fuzzer_execs_per_sec)", "refId": "A" }],
      "options": {
        "reduceOptions": { "calcs": ["last"] },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      }
    },
    {
      "type": "stat",
      "title": "Total Corpus Size",
      "id": 3,
      "gridPos": { "x": 12, "y": 1, "w": 6, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [{ "expr": "sum(fuzzer_corpus_size)", "refId": "A" }],
      "options": {
        "reduceOptions": { "calcs": ["last"] },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      }
    },
    {
      "type": "stat",
      "title": "Total Crashes",
      "id": 4,
      "gridPos": { "x": 18, "y": 1, "w": 6, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [{ "expr": "sum(fuzz_crashes_total)", "refId": "A" }],
      "options": {
        "reduceOptions": { "calcs": ["last"] },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      }
    },
    {
      "type": "graph",
      "title": "Aggregate Execution Rate (All Cores)",
      "id": 5,
      "gridPos": { "x": 0, "y": 5, "w": 24, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum(rate(fuzz_executions_total[5s]))",
          "legendFormat": "execs/sec",
          "refId": "A"
        }
      ],
      "lines": true,
      "fill": 3,
      "linewidth": 2,
      "yaxes": [{ "format": "short" }, { "format": "short" }],
      "legend": { "show": true }
    },
    {
      "type": "graph",
      "title": "Average Corpus Growth (entries/sec)",
      "id": 6,
      "gridPos": { "x": 0, "y": 13, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "avg(rate(fuzzer_corpus_size[5m]))",
          "legendFormat": "avg corpus growth",
          "refId": "A"
        }
      ],
      "lines": true,
      "fill": 3,
      "linewidth": 2,
      "yaxes": [{ "format": "short" }, { "format": "short" }],
      "legend": { "show": true }
    },
    {
      "type": "graph",
      "title": "Average Coverage Growth (edges/sec)",
      "id": 7,
      "gridPos": { "x": 12, "y": 13, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "avg(rate(fuzzer_coverage_unique_edges[5m]))",
          "legendFormat": "avg unique edges/sec",
          "refId": "A"
        }
      ],
      "lines": true,
      "fill": 3,
      "linewidth": 2,
      "yaxes": [{ "format": "short" }, { "format": "short" }],
      "legend": { "show": true }
    },
    {
      "type": "row",
      "title": "Per-Core Breakdown",
      "id": 8,
      "gridPos": { "x": 0, "y": 21, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "graph",
      "title": "Execution Rate by Core",
      "id": 9,
      "gridPos": { "x": 0, "y": 22, "w": 24, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(fuzz_executions_total[5s])",
          "legendFormat": "core={{core}}",
          "refId": "A"
        }
      ],
      "lines": true,
      "fill": 3,
      "linewidth": 2,
      "legend": { "show": true }
    },
    {
      "type": "graph",
      "title": "Corpus Size by Core",
      "id": 10,
      "gridPos": { "x": 0, "y": 30, "w": 24, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "fuzzer_corpus_size",
          "legendFormat": "core={{core}}",
          "refId": "A"
        }
      ],
      "lines": true,
      "fill": 2,
      "linewidth": 2,
      "legend": { "show": true }
    }
  ]
}
