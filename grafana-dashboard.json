{
  "id": null,
  "uid": "fuzz-monitor",
  "title": "LibFuzzer Harness Metrics",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "2s",
  "panels": [
    {
      "type": "stat",
      "title": "Total Fuzz Executions",
      "id": 1,
      "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "fuzz_executions_total",
          "legendFormat": "executions",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["last"], "fields": "", "values": false },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      }
    },
    {
      "type": "graph",
      "title": "Execution Rate (execs/sec)",
      "id": 2,
      "gridPos": { "x": 6, "y": 0, "w": 18, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(fuzz_executions_total[5s])",
          "legendFormat": "rate(5s)",
          "refId": "A"
        }
      ],
      "lines": true,
      "fill": 3,
      "linewidth": 2,
      "pointradius": 1,
      "yaxes": [{ "format": "short", "logBase": 1 }, { "format": "short" }],
      "legend": { "show": true, "values": false }
    },
    {
      "type": "graph",
      "title": "Crash Count",
      "id": 3,
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "fuzz_crashes_total",
          "legendFormat": "crashes",
          "refId": "A"
        }
      ],
      "lines": true,
      "fill": 1,
      "linewidth": 2,
      "yaxes": [{ "format": "short", "logBase": 1 }, { "format": "short" }],
      "legend": { "show": true }
    },
    {
      "type": "graph",
      "title": "Corpus Size",
      "id": 4,
      "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "fuzzer_corpus_size",
          "legendFormat": "corpus",
          "refId": "A"
        }
      ],
      "lines": true,
      "fill": 2,
      "linewidth": 2,
      "yaxes": [{ "format": "short", "logBase": 1 }, { "format": "short" }],
      "legend": { "show": true }
    },
    {
      "type": "stat",
      "title": "Current Execution Speed",
      "id": 5,
      "gridPos": { "x": 0, "y": 16, "w": 6, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "fuzzer_execs_per_sec",
          "legendFormat": "exec/s",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["last"], "fields": "", "values": false },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      }
    }
  ],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-5m",
    "to": "now"
  }
}
